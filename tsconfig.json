{
  "compilerOptions": {
    "allowJs": true,
    "target": "ES2017",
    "module": "ES2022",
    "lib": [
      "es2022",
      "DOM",
      "WebWorker",
      "DOM.Iterable"
    ],
    "lib": [
      "es2022",
      "DOM",
      "WebWorker",
      "DOM.Iterable"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "@builder.io/qwik",
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "incremental": true,
    "isolatedModules": true,
    "outDir": "tmp",
    "noEmit": true,
    "types": [
      "node",
      "vite/client"
    ],
    "types": [
      "node",
      "vite/client"
    ],
    "paths": {
      "~/*": [
        "./src/*"
      ]
      "~/*": [
        "./src/*"
      ]
    }
  },
  "files": [
    "./.eslintrc.cjs",
    "src/routes/work/type/penyerahan-sampah.type.tsx",
    "src/routes/work/penyerahan-sampah/component/transaksi/TotalMaterial.tsx",
    "src/routes/work/penyerahan-sampah/component/OpsiPenukaran.tsx",
    "src/routes/work/penyerahan-sampah/component/Riwayat.tsx",
    "src/routes/work/penyerahan-sampah/component/Profile.tsx",
    "src/routes/work/penyerahan-sampah/component/transaksi/Transaksi.tsx",
    "./src/routes/work/penyerahan-sampah/component/transaksi/TransaksiSampah.tsx",

  ],
  "include": [
    "src",
    "./*.d.ts",
    "./*.config.ts"
  ]
}